{
    "builds": [
        { "src": "server/api/index.js", "use": "@vercel/node" },
        { "src": "client/package.json", "use": "@vercel/static-build" }
    ],
    "routes": [
        { "src": "/api/(.*)", "dest": "server/api/index.js" },
        { "src": "/(.*)", "dest": "client/build/$1" }
    ]
}
